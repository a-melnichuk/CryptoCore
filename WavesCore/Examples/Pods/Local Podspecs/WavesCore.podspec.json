{
  "name": "WavesCore",
  "module_name": "WavesCore",
  "version": "0.0.4",
  "swift_versions": "5.0",
  "summary": "Waves crypto components for Paytomat Wallet",
  "description": "Waves address generation and transaction for Paytomat Wallet",
  "homepage": "https://paytomat.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Alex Melnichuk": "a.melnichuk@noisyminer.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/a-melnichuk/CryptoCore.git",
    "branch": "master",
    "tag": "waves-0.0.4"
  },
  "source_files": [
    "WavesCore/*.h",
    "WavesCore/Sources/*.swift",
    "WavesCore/Sources/libs/paytomat_crypto_core/*.h",
    "WavesCore/Sources/paytomat_waves_core/{include,src}/*.{h,c}"
  ],
  "public_header_files": "WavesCore/*.h",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": [
      "$(PODS_TARGET_SRCROOT)/WavesCore/Sources/**",
      "$(PODS_TARGET_SRCROOT)/WavesCore/Sources/libs",
      "$(PODS_ROOT)/WavesCore/Sources/**",
      "$(PODS_ROOT)/WavesCore/Sources/libs"
    ],
    "SYSTEM_HEADER_SEARCH_PATHS": "$(PODS_TARGET_SRCROOT)/WavesCore/Sources/libs $(PODS_ROOT)/WavesCore/Sources/libs"
  },
  "preserve_paths": "WavesCore/Sources/module.modulemap",
  "exclude_files": "Examples/*",
  "frameworks": "Foundation",
  "dependencies": {
    "CryptoCore": [
      "~> 0.0.8"
    ]
  },
  "swift_version": "5.0"
}
